extend type Query {
    user: User
    userById(id: ID): User
    allUsers: [User]
}

type UsersOutput {
    id: ID!
    username: String!
}

type User {
    id: ID!
    email: String
    emailVerified: Boolean!
    enabled: Boolean!
    firstName: String
    lastName: String
    username: String!
    createdAt: Int!
}
